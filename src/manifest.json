{
  "name": "Spotify Web Now Playing",
  "description": "Export Spotify Web “now playing” song to a local file",
  "version": "0.1.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "incognito": "split",
  "content_scripts": [
    {
      "matches": [
        "https://open.spotify.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "https://open.spotify.com/*"
  ]
}
